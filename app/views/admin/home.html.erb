<center>
	<h1>Applications</h1>
</center>
<% @application_info.each do |application| %>
	<div class="card">
	  <div class="card-body">
	    <ul style="list-style-type: none;">
	    	<li><%= application.full_name %></li>
	    	<li><%= application.city %></li>
	    	<li><%= application.email %> </li>
	    </ul>
	    <center>
		    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#viewApplicationModal">
		      View
		    </button>
		</center>
	  </div>
	</div>
	<%= render "view_application", application: application %> 
<% end %>
